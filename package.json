{
  "name": "course-commander",
  "description": "An app for planning out college schedules",
  "version": "0.0.1",
  "private": true,
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "storybook": "NODE_PATH=src start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-checkout": "yarn install --frozen-lockfile",
      "post-merge": "yarn install --frozen-lockfile",
      "post-rewrite": "yarn install --frozen-lockfile"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "autoprefixer": "^9.6.0",
    "compression": "^1.7.1",
    "fast-deep-equal": "^2.0.1",
    "husky": "^2.7.0",
    "immer": "^8.0.1",
    "js-joda": "^1.11.0",
    "lint-staged": "^8.2.1",
    "polka": "^0.5.0",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "sirv": "^0.4.0",
    "svelte-preprocess": "^2.14.1",
    "tailwindcss": "^1.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.17",
    "prettier": "^1.18.2",
    "rollup": "^1.16.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.0.0",
    "sapper": "^0.27.0",
    "svelte": "^3.6.2"
  }
}
